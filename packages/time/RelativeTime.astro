---
import Import from './Import.astro';

export interface Props extends astroHTML.JSX.AnchorHTMLAttributes {
    datetime: Date | string;
}

const {
  datetime,
  ...attrs
} = Astro.props
---

<Import />

<relative-time
  datetime={(datetime instanceof Date)?datetime.toISOString():datetime}
  {...attrs}
>
  <slot />
</relative-time>